{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true,
      "tsx": true
    },
    "transform": {
      "react": {
        "pragma": "h",
        "pragmaFrag": "Fragment",
        "importSource": "preact",
        "runtime": "automatic"
      }
    },
    "baseUrl": "./"
  },
  "minify": false
}
